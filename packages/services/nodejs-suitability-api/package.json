  "bugs": {
    "url": "https://github.com/vitor-tadashi/suitability-api/issues"
  },
  "homepage": "https://github.com/vitor-tadashi/suitability-api#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.2.0",
    "swagger-ui-express": "^4.1.5"
  }
}
