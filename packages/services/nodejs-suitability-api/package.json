{
  "name": "nodejs-suitability-api",
  "version": "1.0.0",
  "description": "Suitability API in NodeJS",
  "main": "./src/server.js",
  "scripts": {
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
    "dev": "cross-env RUN_ENV=dev nodemon -w server -w build -w src ./src/server.js",
    "test": "cross-env RUN_ENV=test jest --ci --coverage --runInBand --detectOpenHandles --useStderr --silent",
    "start": "node ./src/server.js"
  },
  "repository": {
    "type": "git",
